{
  "extends": "./tsconfig.paths",
  "name": "gersang-supporter",
  "version": "0.1.0",
  "private": true,
  "author": "Paosder",
  "dependencies": {
    "electron-debug": "^3.0.1",
    "fs-jetpack": "^2.2.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "regedit": "^3.0.3",
    "winax": "^1.13.0"
  },
  "homepage": "./",
  "scripts": {
    "dev": "concurrently \"yarn front:start\" \"yarn electron:dev\"",
    "front:start": "craco start",
    "electron:dev": "tsc -w main/electron.ts --outDir build",
    "electron:start-prod": "cross-env NODE_ENV=production electron ./build/electron.js",
    "electron:build": "tsc main/electron.ts --outDir build",
    "start": "cross-env NODE_ENV=development ELECTRON_START_URL=http://localhost:3000 nodemon --watch build/electron.js --exec \"electron ./build/electron.js\"",
    "build:win64": "electron-builder build --win --x64",
    "build:front": "craco build",
    "build:native": ".\\node_modules\\.bin\\electron-rebuild.cmd",
    "build": "cross-env NODE_ENV=production yarn build:front && yarn electron:build && yarn build:win64",
    "test": "craco test"
  },
  "build": {
    "productName": "Gersang Supporter",
    "appId": "com.gersangsupporter.app",
    "copyright": "Copyright Â© 2020 ${author}",
    "asar": true,
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./public/logo512.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "./public",
      "output": "release/",
      "app": "."
    },
    "files": [
      "!**/node_modules/@babel${/*}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@craco/craco": "^5.6.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.2",
    "copyfiles": "^2.1.1",
    "cross-env": "^6.0.3",
    "electron": "^7.1.9",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.8.8",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react-perf": "^3.2.3",
    "nodemon": "^2.0.2",
    "react-scripts": "3.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "~3.7.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
